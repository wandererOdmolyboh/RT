int i = 0;
			double r,g,b;
			int tmp;
			while (HEIGH * WIDTH > i)
			{
				tmp = img.pixels[i];
				r = (((tmp >> 16) & 0xFF) * 0.393) + (((tmp >> 8) & 0xFF) *0.769) + ((tmp & 0xFF) * 0.189);
				g = (((tmp >> 16) & 0xFF) * 0.349) + (((tmp >> 8) & 0xFF) *0.686) + ((tmp & 0xFF) * 0.168);
				b = (((tmp >> 16) & 0xFF) * 0.272) + (((tmp >> 8) & 0xFF) *0.534) + ((tmp & 0xFF) * 0.131);
				img.pixels[i] = get_color((int)r, (int)g, (int)b);
				i++;
			}